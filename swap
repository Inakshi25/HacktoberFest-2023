<script> 
	// Javascript program to swap two numbers 
	// without using a temporary variable 
	var x = 10; 
	var y = 5; 

	console.log("Before swapping:" + " x = " + 
		x + ", y = " + y); 

	// Code to swap 'x' and 'y' 
	x = x * y; // x now becomes 50 
	y = x / y; // y becomes 10 
	x = x / y; // x becomes 5 

	console.log("After swapping:" + " x = " + 
		x + ", y = " + y); 
</script> 
